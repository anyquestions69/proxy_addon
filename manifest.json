{
    "manifest_version": 3,
    "name": "proxy_addon",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
    "permissions": [
        "webRequest",
        "http://localhost*"
      ],
    "background": {
        "scripts": ["proxy.js"]
       }
  }
  